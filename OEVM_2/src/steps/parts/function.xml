                            <m:t>4</m:t>
                        </m:r>
                    </m:sub>
                </m:sSub>
            </m:e>
        </m:d>
        <m:r>
            <w:rPr>
                <w:rFonts w:ascii="Cambria Math" w:hAnsi="Cambria Math"/>
            </w:rPr>
            <m:t>=</m:t>
        </m:r>

        {% for con in function %}
            {% for var in con %}
                {% if var != '-' %}
                    {% if var == '0' %}
                    <m:bar>
                        <m:barPr>
                            <m:pos m:val="top"/>
                            <m:ctrlPr>
                                <w:rPr>
                                    <w:rFonts w:ascii="Cambria Math" w:hAnsi="Cambria Math"/>
                                    <w:i/>
                                </w:rPr>
                            </m:ctrlPr>
                        </m:barPr>
                        <m:e>
                    {% endif %}
                    <m:sSub>
                        <m:sSubPr>
                            <m:ctrlPr>
                                <w:rPr>
                                    <w:rFonts w:ascii="Cambria Math" w:hAnsi="Cambria Math"/>
