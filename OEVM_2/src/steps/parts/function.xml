                        </m:ctrlPr>
                    </m:sSubPr>
                    <m:e>
                        <m:r>
                            <w:rPr>
                                <w:rFonts w:ascii="Cambria Math" w:hAnsi="Cambria Math"/>
                            </w:rPr>
                            <m:t>x</m:t>
                        </m:r>
                    </m:e>
                    <m:sub>
                        <m:r>
                            <w:rPr>
                                <w:rFonts w:ascii="Cambria Math" w:hAnsi="Cambria Math"/>
                            </w:rPr>
                            <m:t>2</m:t>
                        </m:r>
                    </m:sub>
                </m:sSub>
                <m:r>
                    <w:rPr>
                        <w:rFonts w:ascii="Cambria Math" w:hAnsi="Cambria Math"/>
                    </w:rPr>
                    <m:t>,</m:t>
                </m:r>
