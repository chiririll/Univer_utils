                                <w:rPr>
                                    <w:rFonts w:ascii="Cambria Math" w:hAnsi="Cambria Math"/>
                                </w:rPr>
                                <m:t>{{ loop.index }}</m:t>
                            </m:r>
                        </m:sub>
                    </m:sSub>
                    {% if var == '0' %}
                        </m:e>
                    </m:bar>
                    {% endif %}
                {% endif %}
            {% endfor %}
            {% if loop.index != function|length %}
            <m:r>
                <w:rPr>
                    <w:rFonts w:ascii="Cambria Math" w:hAnsi="Cambria Math"/>
                </w:rPr>
                <m:t>âˆ¨</m:t>
            </m:r>
            {% endif %}
        {% endfor %}
