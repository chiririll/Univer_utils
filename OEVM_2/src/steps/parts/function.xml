<m:oMathPara>
    <m:oMath>
        {% if not func_index %}
        <m:r>
            <w:rPr>
                <w:rFonts w:ascii="Cambria Math" w:hAnsi="Cambria Math"/>
            </w:rPr>
            <m:t>f</m:t>
        </m:r>
        {% else %}
        <m:sSub>
            <m:sSubPr>
                <m:ctrlPr>
                    <w:rPr>
                        <w:rFonts w:ascii="Cambria Math" w:hAnsi="Cambria Math"/>
