<w:p>
    <w:pPr>
        <w:ind w:firstLine="{{ image.get(first_line)|default(0) }}"/>
        <w:jc w:val="{{ image.get(align)|default('center') }}"/>
    </w:pPr>
    <w:r>
        <w:pict>
            <v:shape {% if 'style' in image.keys() %}style="width:{{ imagewidth }};height:{{ height }}">
                <v:imagedata r:id="rId{{ rel.id }}"/>
            </v:shape>
        </w:pict>
    </w:r>
</w:p>